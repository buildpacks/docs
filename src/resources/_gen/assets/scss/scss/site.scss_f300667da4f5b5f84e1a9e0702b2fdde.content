/**********************************

Header

**********************************/
/**** mobile nav ****/
header ul {
  display: none; }

header input[type=checkbox] {
  display: none; }

header {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20px 40px; }

/* toggle */
header #mobile-nav-toggle ~ label {
  font-size: 1.9rem;
  transition: opacity 0.3s;
  margin-bottom: 0; }

header #mobile-nav-toggle:checked ~ label {
  opacity: 0.5; }

header #mobile-nav-toggle:checked ~ label ~ ul {
  width: 100%;
  display: flex; }

header ul {
  list-style-type: none;
  margin: 0;
  padding: 0;
  background: white;
  justify-content: space-between;
  margin-top: 20px; }

header li {
  display: flex;
  align-items: center;
  justify-content: center;
  padding-left: 10px; }
  header li:first-child {
    padding-left: 0; }

header li a {
  white-space: nowrap;
  text-decoration: none; }
  header li a:hover {
    text-decoration: inherit; }
  header li a:not(.button) {
    border-bottom: 2px solid rgba(71, 82, 158, 0.25); }
  header li a.active {
    border-bottom-color: #47529E; }

/**** desktop nav ****/
@media (min-width: 768px) {
  header #mobile-nav-toggle ~ label {
    display: none; }
  header ul {
    display: flex;
    margin-top: 0; }
  header li {
    padding-left: 40px; }
    header li:last-of-type {
      margin-right: 0; } }

/**** general header styles ****/
header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20px 30px; }

header .logo img {
  width: auto;
  min-width: 160px; }
  @media (min-width: 768px) {
    header .logo img {
      width: 200px; } }

/**********************************

Hero

**********************************/
.hero {
  padding: 8vw 20px;
  text-align: center; }

.hero h1 {
  font-weight: 600;
  max-width: 580px;
  margin: 0 auto 20px; }

.hero .button-holder {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-bottom: 0; }

.hero .button {
