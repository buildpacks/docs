  background-image: url("/images/download-icon.svg"); }

/**********************************

Code

**********************************/
.pre-scrollable {
  max-height: 340px;
  overflow-y: scroll; }

/* one line code */
code.highlight {
  background-color: #EFEFEF;
  padding: 5px 10px;
