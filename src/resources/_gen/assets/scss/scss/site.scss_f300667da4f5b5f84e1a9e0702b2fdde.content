/* multi-line code */
.code-block {
  background-color: white;
  border: 1px solid rgba(124, 110, 117, 0.25);
  border-radius: .25rem;
  padding: 15px 20px; }

/* command line code (inludes an not-selectable $ at the beginning) */
.command-line::before {
  content: '$';
  color: rgba(124, 110, 117, 0.5);
  margin-right: 15px; }

